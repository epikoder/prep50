{
  "name": "Objective",
  "table": "objectives",
  "uniqueId": "id",
  "attributes": [
    {
      "field": "id",
      "type": "bigint"
    },
    {
      "field": "subject_id",
      "type": "relation",
      "target": "subjects:id",
      "collection": "subject",
      "displayName": "Subject",
      "relation": {
        "type": "oneToMany",
        "reference": "forward",
        "column": "name"
      }
    },
    {
      "field": "topic",
      "type": "join-relation",
      "collection": "topic",
      "relation": "belongsTo",
      "tableName": "topics",
      "joinTableName": "topic_objectives",
      "joinForeignKey": "objective_id",
      "joinReferenceKey": "topic_id",
      "displayName": "Topic",
      "column": "title"
    },
    {
      "field": "title",
      "type": "string"
    },
    {
      "field": "details",
      "type": "text"
    },
    {
      "field": "tag",
      "type": "enum",
      "displayName": "Jamb / Waec",
      "values": {
        "Jamb": "J",
        "Waec": "W",
        "Jamb & Waec": "JW"
      }
    },
    {
      "field": "question",
      "type": "join-relation",
      "relation": "hasMany",
      "tableName": "questions",
      "collection": "question",
      "joinTableName": "objective_questions",
      "joinForeignKey": "objective_id",
      "joinReferenceKey": "question_id",
      "displayName": "Questions"
    },
    {
      "field": "lesson",
      "type": "join-relation",
      "relation": "hasMany",
      "tableName": "lessons",
      "collection": "lesson",
      "joinTableName": "objective_lessons",
      "joinForeignKey": "objective_id",
      "joinReferenceKey": "lesson_id",
      "displayName": "Lessons"
    },
    {
      "field": "__edit",
      "type": "action",
      "action": "edit",
      "displayName": "Edit"
    },
    {
      "field": "__delete",
      "type": "action",
      "action": "delete",
      "displayName": "Delete"
    }
  ]
}
